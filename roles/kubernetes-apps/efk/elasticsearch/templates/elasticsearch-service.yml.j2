---
apiVersion: v1
kind: Service
metadata:
  name: elasticsearch-logging
  namespace: "kube-system"
  labels:
    k8s-app: elasticsearch-logging
    kubernetes.io/cluster-service: "true"
    kubernetes.io/name: "Elasticsearch"
spec:
  ports:
  - port: {{ elasticsearch_service_port }}
    protocol: TCP
    targetPort: db
  selector:
    k8s-app: elasticsearch-logging

