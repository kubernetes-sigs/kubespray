{% for cilium_bgp_advertisement in cilium_bgp_advertisements %}
---
apiVersion: cilium.io/v2
kind: CiliumBGPAdvertisement
metadata:
  name: "{{ cilium_bgp_advertisement.name }}"
{% if cilium_bgp_advertisement.labels is defined and cilium_bgp_advertisement.labels %}
  labels:
{{ cilium_bgp_advertisement.labels | to_yaml | indent(4) }}
{% endif %}
spec:
{{ cilium_bgp_advertisement.spec | to_yaml | indent(2) }}
{% endfor %}
